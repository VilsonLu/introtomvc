@model IEnumerable<ProjectMVC.Models.Restaurant>


@{
    ViewBag.Title = "Home Page";
}

<div>
    <form method="get">
        <input type="search" name="searchTerm"/>
        <input type="submit" />
    </form>
</div>


@foreach (var item in Model)
{
    <div class="restaurant">
        <h4>@item.Name</h4>
        <div>@item.City, @item.Country</div>
    </div>
}